function filterCmd($text){
	if(strstr($text,'/')){
		while(strstr($text,'/')){
			$text=str_replace('/','',$text);
		}
	}
	if(strstr($text,'!')){
		while(strstr($text,'!')){
			$text=str_replace('!','',$text);
		}
	}
	if(strstr($text,'#')){
		while(strstr($text,'#')){
			$text=str_replace('#','',$text);
		}
	}
	if(preg_match('/^[A-Z]/',$text)){
		$text=strtolower($text);
	}
	return $text;
}
