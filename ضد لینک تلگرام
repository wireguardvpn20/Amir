			[['text'=>"$oders[highest_number_character_constraint]",'callback_data'=>"none"],['text'=>"ðŸ“‰| Ø¨ÛŒØ´ØªØ±ÛŒÙ† ØªØ¹Ø¯Ø§Ø¯",'callback_data'=>"none"]],
			[['text'=>"10",'callback_data'=>"high_c_constraint_1-"],['text'=>"âž•10",'callback_data'=>"high_c_constraint_1+"]],
			[['text'=>"100",'callback_data'=>"high_c_constraint_2-"],['text'=>"âž•100",'callback_data'=>"high_c_constraint_2+"]],
			[['text'=>"Ø®Ø§Ù†Ù‡ ðŸ›",'callback_data'=>"managemant_group"]]
			]]);
		}else{
			return json_encode(['resize_keyboard'=>true,'inline_keyboard'=>[
			[['text'=>"$oders[character_constraint]",'callback_data'=>"character_constraint"],['text'=>"ðŸ“­| Ù…Ø­Ø¯ÙˆØ¯ÛŒØª Ú©Ø§Ø±Ø§Ú©ØªØ±",'callback_data'=>"none"]],
			[['text'=>"Ø®Ø§Ù†Ù‡ ðŸ›",'callback_data'=>"managemant_group"]]
			]]);
		}
	}else{
		if($oders['character_constraint']=='âœ…'){
			return json_encode(['resize_keyboard'=>true,'inline_keyboard'=>[
			[['text'=>"ðŸ“­| Character constraint",'callback_data'=>"none"],['text'=>"$oders[character_constraint]",'callback_data'=>"character_constraint"]],
			[['text'=>"ðŸ“‰| Highest number",'callback_data'=>"none"],['text'=>"$oders[highest_number_character_constraint]",'callback_data'=>"none"]],
			[['text'=>"10",'callback_data'=>"high_c_constraint_1-"],['text'=>"âž•10",'callback_data'=>"high_c_constraint_1+"]],
			[['text'=>"100",'callback_data'=>"high_c_constraint_2-"],['text'=>"âž•100",'callback_data'=>"high_c_constraint_2+"]],
			[['text'=>"Back ðŸ›",'callback_data'=>"managemant_group"]]
			]]);
		}else{
			return json_encode(['resize_keyboard'=>true,'inline_keyboard'=>[
			[['text'=>"ðŸ“­| Character constraint",'callback_data'=>"none"],['text'=>"$oders[character_constraint]",'callback_data'=>"character_constraint"]],
			[['text'=>"Back ðŸ›",'callback_data'=>"managemant_group"]]
			]]);
		}
	}
}
